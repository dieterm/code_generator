<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>{{ DotNetProject.template.dot_net_target_framework }}</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
{{- for package in DotNetProject.packages}}
	  <PackageReference Include="{{ package.package_id }}" Version="{{ package.version }}" />
{{- end}}
  </ItemGroup>
</Project>

<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>{{ DotNetProject.template.dot_net_target_framework.dot_net_command_line_argument }}</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
{{- for package in DotNetProject.packages }}
	  <PackageReference Include="{{ package.package_id }}" Version="{{ package.version }}" />
{{- end }}
  </ItemGroup>

  <ItemGroup>
{{- for projectRef in ProjectReferences }}
    <ProjectReference Include="{{ projectRef }}" />
{{- end }}
  </ItemGroup>
</Project>